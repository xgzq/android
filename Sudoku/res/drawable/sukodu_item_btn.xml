<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/sudoku_item_btn_corners"/>
            <solid android:color="@color/sudoku_item_btn_number_normal"/>
            <stroke android:width="@dimen/sudoku_item_btn_dash"
                android:color="@color/sudoku_item_btn_dash"
                android:dashWidth="10dp"
                android:dashGap="0dp"/>
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/sudoku_item_btn_corners"/>
            <solid android:color="@color/sudoku_item_btn_number_pressed"/>
        </shape>
    </item>
</selector>
